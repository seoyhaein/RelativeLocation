<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- VisualBrush -->
    <VisualBrush x:Key="SmallGridLinesBrush" TileMode="Tile" SourceRect="0,0,20,20" DestinationRect="0,0,20,20">
        <VisualBrush.Visual>
            <Path Stroke="{DynamicResource GridLinesBrush}" StrokeThickness="1" Data="M 0,0 L 0,20 L 20,20 L 20,0 Z" />
        </VisualBrush.Visual>
    </VisualBrush>
</ResourceDictionary>